<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="order_1" class="com.aditya.spring.order.Order">
		<property name="id" value="1" />
		<property name="item" ref="item_1" />
	</bean>

	<bean id="item_1" class="com.aditya.spring.order.Item">
		<property name="id" value="1" />
		<property name="name" value="item_1" />
		<property name="price" value="100" />
	</bean>

	<bean id="order_2" class="com.aditya.spring.order.Order">
		<property name="id" value="2" />
		<property name="item" ref="item_2" />
	</bean>

	<bean id="item_2" class="com.aditya.spring.order.Item">
		<property name="id" value="2" />
		<property name="name" value="item_2" />
		<property name="price" value="200" />
	</bean>


	<bean id="order_3" class="com.aditya.spring.order.Order">
		<property name="id" value="3" />
		<property name="item" ref="item_3" />
	</bean>

	<bean id="item_3" class="com.aditya.spring.order.Item">
		<property name="id" value="3" />
		<property name="name" value="item_3" />
		<property name="price" value="300" />
	</bean>



</beans>